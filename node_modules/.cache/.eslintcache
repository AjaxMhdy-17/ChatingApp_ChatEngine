  </Link>\n                        <ButtonContainer \n                            cart\n                            disabled={product.inCart? true : false}\n                            onClick={() => {\n                                ctx.addToCart(product.id)\n                                ctx.openModal(product.id) \n                            }}\n                        >\n                            {product.inCart ? \"in cart\":\"add to cart\"}\n                        </ButtonContainer>\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default ProductDetail\n","/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/components/Cart/CartTotal.js",[],"/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/components/Cart/CartList.js",[],"/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/components/Cart/CartColumn.js",[],"/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/components/UI/Title.js",[],"/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/components/Cart/EmptyCart.js",[],"/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/components/Cart/CartItem.js",[],"/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/components/Cart/CheckOut.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":4,"column":9,"nodeType":"101","messageId":"102","endLine":4,"endColumn":24},{"ruleId":"99","severity":1,"message":"103","line":5,"column":43,"nodeType":"101","messageId":"102","endLine":5,"endColumn":49},{"ruleId":"104","severity":1,"message":"105","line":24,"column":26,"nodeType":"106","endLine":24,"endColumn":100},{"ruleId":"99","severity":1,"message":"107","line":27,"column":11,"nodeType":"101","messageId":"102","endLine":27,"endColumn":24},{"ruleId":"104","severity":1,"message":"105","line":25,"column":29,"nodeType":"106","endLine":25,"endColumn":94},{"ruleId":"104","severity":1,"message":"105","line":22,"column":21,"nodeType":"106","endLine":22,"endColumn