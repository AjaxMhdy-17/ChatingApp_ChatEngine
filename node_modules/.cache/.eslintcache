st Product = (props) => {\n\n    const product = props.product\n    // console.log(product);\n\n    const ctx = UseMainContext() ;\n    // console.log(ctx);\n\n\n    return (\n        <ProductWrapper className='my-3'>\n            <div className=\"card\">\n                \n                <div\n                 onClick={() => ctx.getDetail(product.id)} \n                 className=\"img__container p-5\">\n                     <Link to='/detail'>\n                         <img src={product.img} className='card__img__top img-fluid' alt=\"image\" />\n                     </Link> \n                     <button\n                        disabled={product.inCart? true : false}\n                        onClick={() => {\n                            ctx.openModal(product.id)\n                            ctx.addToCart(product.id)\n                            }\n                        }\n                        className=\"cart__btn\"\n                     >\n                         {\n                              product.inCart ? (\n                                  <p className=\"text-capitalize mb-0\" disabled>\n                                      in Cart\n                                  </p>\n                              ) : (\n                                <FaCartPlus/>\n                              )\n                         }\n                     </button>\n                </div>\n\n                <div className=\"card-footer d-flex justify-content-between\">\n                    <p className=\"align-self-center mb-0\">\n                        {product.title}\n                    </p>\n                    <h5 className=\"text-blue font-italic mb-0\">\n                        <span className=\"mr-1\">$</span>\n                        {product.price}\n                    </h5>\n                </div>\n\n            </div>\n        </ProductWrapper>\n    )\n}\n\nconst ProductWrapper = styled.div`\n    .card{\n        border-color: transparent;\n        transition: all 1s linear;\n\n        .img__container{\n            position : relative ;\n            overflow : hidden ;\n\n            .card__img__top{\n                transition : all 1s ease-in-out ;\n            }\n\n            .cart__btn {\n                position: absolute;\n                bottom: 0;\n                right: 0;\n                padding: 0.2rem 0.4rem;\n                background: var(--lightBlue);\n                border: none;\n                color: var(--mainWhite);\n                font-size: 1.4rem;\n                border-radius: 0.5rem 0 0 0;\n                transform: translate(100%, 100%);\n                transition: all 1s ease-in-out;\n\n                &:hover{\n                    color : var(--mainBlue) ;\n                    cursur : pointer ;\n                }\n\n              }\n\n            &:hover{\n                .card__img__top{\n                    transform : scale(1.2) \n                }\n                .cart__btn{\n                    transform : translate(0,0)\n                }\n            }\n\n        }\n\n\n        .card-footer {\n            background: transparent;\n            border-top: transparent;\n            transition: all 1s linear;\n        }\n        &:hover{\n            border: 0.04rem solid rgba(0, 0, 0, 0.2);\n            box-shadow: 2px 2px 5px 0px rgba(0, 0, 0, 0.2);\n            .card-footer{\n                background: rgba(247, 247, 247);\n            }\n        }\n    }\n`\n\nexport default Product\n","/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/context/Context.js",["92"],"/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/data.js",[],"/home/mhdy/Music/js/react/Practice/src/phoneEcmrs/components/Modal/Modal.js",["93"],"import React from 'react'\nimport styled from 'styled-components';\nimport { UseMainContext } from '../../context/Context'\nimport { ButtonContainer } from '../UI/ButtonContainer';\nimport { Link } from 'react-router-dom';\n\nfunction Modal() {\n\n    const ctx = 